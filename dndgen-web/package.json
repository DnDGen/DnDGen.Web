{
  "name": "dndgen-web",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44485 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44485 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "build:ci": "ng build --production",
    "build:ssr": "ng run DnDGen.Web:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "jasmine",
    "test:dummy": "jest",
    "test:ci": "ng build && jasmine",
    "local": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "bootstrap": "^5.0.0",
    "bootstrap-icons": "^1.0.0",
    "jquery": "^3.6.4",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0",
    "sweetalert2": "^11.7.27",
    "file-saver": "^2.0.5",
    "@types/file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^20.0.0",
    "@jest/globals": "^29.7.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3"
  },
  "overrides": {
    "autoprefixer": "10.4.5",
    "webpack": "5.81.0"
  },
  "optionalDependencies": {}
}
